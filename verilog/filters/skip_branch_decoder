# Control bits for skip branch decoder
#     !NZL
0000000000 ?cyan4?never
0000000001 L
0000000010 Z
0000000011 Z+L
0000000100 N
0000000101 N+L
0000000110 N+Z
0000000111 N+Z+L
0000001000 ?green4?always
0000001001 ~L
0000001010 ~Z
0000001011 ~Z~L
0000001100 ~N
0000001101 ~N~L
0000001110 ~N~Z
0000001111 ~N~Z~L
